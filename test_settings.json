      "delete_s3_zip": false
    },
    "build_package_only_delete_local_zip_true": {
      "delete_local_zip": true,
      "use_precompiled_packages": false,
      "delete_s3_zip": false
    },
    "lambda_concurrency_enabled": {
        "extends": "ttt888",
